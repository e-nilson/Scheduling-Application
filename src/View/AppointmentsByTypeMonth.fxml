            <TableColumn fx:id="monthCol" prefWidth="200.0" text="Month" />
            <TableColumn fx:id="typeCol" prefWidth="200.0" text="Type of Appointment" />
            <TableColumn fx:id="totalCol" prefWidth="200.0" text="Total Number of Appointments" />
         </columns>
      </TableView>
      <Text layoutX="209.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Appointments by Month Report" />
