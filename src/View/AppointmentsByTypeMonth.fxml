      <ComboBox fx:id="monthComboBox" layoutX="212.0" layoutY="53.0" onAction="#onMonthComboBox" prefHeight="25.0" prefWidth="131.0" promptText="Select Month" />
      <Button fx:id="viewMonthButton" layoutX="364.0" layoutY="53.0" mnemonicParsing="false" onAction="#onViewMonthButton" text="View" />
